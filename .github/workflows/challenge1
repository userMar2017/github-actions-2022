
name: Complex Workflow Challenge

on: [push]

jobs:

  Ubuntu job:

    name:  Ubuntu job
    runs-on:  ubuntu-latest
    steps:
    - name: Ubuntu date
      run: date

  Windows job:

    name:  Windows job
    runs-on:  windows-latest
    - name: Windows date
      run: date

  MacOS job:

    name:  MacOS job
    runs-on:  macos-latest
    - name: Ubuntu date
      run: date
      
  - name:  Ubuntu job
    runs-on:  ubuntu-latest
    - name: Ubuntu date
      run: date    
    
    
    
    
